{
  "name": "ai-ticket-system",
  "version": "1.0.0",
  "description": "AI-powered issue ticketing system with Node.js, Express, React, and SQLite",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup-db": "node server/scripts/setupDatabase.js"
  },
  "keywords": [
    "ticketing",
    "support", 
    "ai",
    "express",
    "react"
  ],
  "author": "AI Assistant",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.5.0",
    "openai": "^4.11.1",
    "natural": "^6.5.0",
    "compromise": "^14.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1"
  }
}
